# Cài dependencies cho root project
npm install

# Cài dependencies cho frontend
cd frontend
npm install

# Cài dependencies cho backend  
cd ../backend
npm install

#Chạy Cả Frontend + Backend 
# Từ thư mục root
npm start
# Hoặc Chạy Riêng Từng Phần

##Frontend
cd frontend
npm run dev
#Backend
cd backend
npm run dev

